{
  "manifest_version": 3,
  "name": "ContentHub 测试版",
  "version": "1.0",
  "description": "简化版ContentHub插件用于测试",
  "permissions": [
    "activeTab", 
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "test-simple.html",
    "default_title": "ContentHub测试"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
